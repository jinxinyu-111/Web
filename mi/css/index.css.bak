/* 轮播图 */
.banner{
}
.bannerShow{
width:1226px;
overflow:hidden;
}
.bannerCon{
width:6140px;
animation:car 30s linear infinite;
}
.bannerList li{
float:left;
}
.arrow a{
color:#fff;
font-size:58px;
background:#e3e3e3;
position:absolute;
top:180px;
display:block;
height:108px;
line-height:100px;
}
.arrow a:hover{
background:#FF6700;
}
.arrow .arrow_right{
right:0px;
}
.arrow .arrow_left{
left:236px;
}
.minCircle{
position:absolute;
bottom:15px;
right:15px;
}
.minCircle li{
width:10px;
height:10px;
border:3px solid #000;
border-radius:50%;
float:left;
margin:0 3px;
}
@keyframes car{
	0%{
	transform:translate(0px);
	}
	19%{
	transform:translate(0px);
	}
	20%{
	transform:translate(-1226px);
	}
	39%{
	transform:translate(-1226px);
	}
	40%{
	transform:translate(-2452px);
	}
	59%{
	transform:translate(-2452px);
	}
	60%{
	transform:translate(-3678px);
	}
	79%{
	transform:translate(-3678px);
	}
	80%{
	transform:translate(-4904px);
	}
	100%{
	transform:translate(-4904px);
	}
}
.l1{
animation:first 30s linear infinite;
}
@keyframes first{
  0%{
  background:##FF6700;
    }
 19%{
  background:##FF6700;
    }
20%{
	background:#fff;
	}
100%{
